#include<iostream>
using namespace std;
int bs_l(int i,int j,int x);
 int bs(int i,int j,int x);
int bs_r(int i,int j,int x);
int arr[20];
int main()
{
 int n,x,result;
  cin>>n; 
  for(int i=0;i<n;i++)
  {
    cin>>arr[i];
  }
  cout<<"element to search:";
  cin>>x;
  result=bs(0,n,x);
  if(result==-1)
 cout<<result;
return 0;
}

 int bs(int i,int j,int x)
{
    int mid,a,b;

  if(i<=j)
  {
    mid=(i+j)/2;
    if(arr[mid]==x)
    {
	    if(arr[mid-1]!=x)
	     a=mid;
	     else
	     a=bs_l(i,mid-1,x);
   	
    if(arr[mid+1]!=x)
     b=mid;
    else
    b=bs_r(mid+1,j,x);
    cout<<"["<<a<<","<<b<<"]";

    }
    else if(arr[mid]>x)
    bs(i,mid-1,x);
    else
     bs(mid+1,j,x);
  }
  else return -1;
}
 int bs_l(int i,int j,int x)
{
    int mid;

  if(i<=j)
  {
    mid=(i+j)/2;
    if(arr[mid]==x)
    {
    bs_l(i,mid-1,x);
    return mid+1;
    }
    else if(arr[mid]>x)
    bs_l(i,mid-1,x);
    else
     bs_l(mid+1,j,x);
  }
  else return -1;
}

int bs_r(int i,int j,int x)
{
    int mid;

  if(i<=j)
  {
    mid=(i+j)/2;
    if(arr[mid]==x)
    {
    bs_r(i,mid-1,x);
    return mid+1;
    }
    else if(arr[mid]>x)
    bs_r(i,mid-1,x);
    else
     bs_r(mid+1,j,x);
  }
  else return -1;
}
